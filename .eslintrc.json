{
  "parser": "babel-eslint",
  "extends": ["airbnb-base", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": ["error"],
    "no-console": "off"
  },
  "env": {
    "commonjs": true,
    "node": true,
    "jest": true
  },
  "ignorePatterns": ["node_modules/", "coverage/", "lib/", "bin/"]
}
